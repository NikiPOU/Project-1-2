<VBox alignment="CENTER" prefHeight="508.0" prefWidth="785.0" spacing="20.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="twentyone.StartSceneController">
   <children>
      <Label text="A Titanic Space Odyssey!" textFill="WHITE">
         <font>
            <Font name="System Bold" size="33.0" />
         </font></Label>
      <Button fx:id="primaryButton" onAction="#switchToSolar" style="-fx-background-color: white;" text="Start journey">
         <font>
            <Font name="System Bold" size="17.0" />
         </font></Button>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
