<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="508.0" prefWidth="785.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="twentyone.SolarSceneController">
   <children>
        <Button fx:id="secondaryButton" layoutX="14.0" layoutY="14.0" onAction="#switchToStart" style="-fx-background-color: white;" text="Return">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <ImageView fitHeight="90.0" fitWidth="90.0" layoutX="337.0" layoutY="209.0">
         <image>
            <Image url="@sun.png" />
         </image>
      </ImageView>
      <ImageView fx:id="earth" fitHeight="49.0" fitWidth="54.0" layoutX="140.0" layoutY="230.0">
         <image>
            <Image url="@earth.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="18.0" fitWidth="20.0" layoutX="201.0" layoutY="209.0">
         <image>
            <Image url="@moon.png" />
         </image>
      </ImageView>
      <Label fx:id="probeCoords" layoutX="14.0" layoutY="422.0" text="Currect probe coords: " textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="distanceTitan" layoutX="14.0" layoutY="440.0" text="Distance to Titan: " textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="launchCoords" layoutX="14.0" layoutY="458.0" text="Launch coords:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="launchVelocity" layoutX="14.0" layoutY="476.0" text="Launch velocity:" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
